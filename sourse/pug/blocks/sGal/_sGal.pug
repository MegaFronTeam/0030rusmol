mixin sGal(data)
	// start sGal
	+b.sGal.section#sGal&attributes(attributes)
		.container
			+e.slider--js.swiper
				.swiper-wrapper
					- for (var x = 0; x < 5; x++)
						+e.slide.swiper-slide
							+e.img-wrap(href="img/head-page.png" data-fancybox="gal").bg-wrap
								+img('head-page.png','bg').picture-bg
	// end sGal